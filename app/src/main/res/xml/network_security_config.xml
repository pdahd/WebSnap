<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!-- 
        允许明文 HTTP 流量（用于访问 http:// 网站）
    -->
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system" />
        </trust-anchors>
    </base-config>
</network-security-config>
